<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_auth_graph"
    app:startDestination="@id/registrationFragment">

    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.emika.app.presentation.ui.auth.AuthFragment"
        android:label="registration_fragment"
        tools:layout="@layout/auth_fragment" >
        <action
            android:id="@+id/action_registrationFragment_self"
            app:destination="@id/registrationFragment" />
        <action
            android:id="@+id/action_registrationFragment_to_startActivity"
            app:destination="@id/startActivity" />
    </fragment>
    <activity
        android:id="@+id/startActivity"
        android:name="com.emika.app.presentation.ui.StartActivity"
        android:label="activity_start"
        tools:layout="@layout/activity_start" />
</navigation>